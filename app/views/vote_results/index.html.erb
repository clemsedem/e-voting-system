<p id="notice"><%= notice %></p>

<h1>Listing Vote Results</h1>

<table>
  <thead>
    <tr>
      <th>Portfolio</th>
      <th>Voter</th>
      <th>Candidate</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_results.each do |vote_result| %>
      <tr>
        <td><%= vote_result.portfolio_id %></td>
        <td><%= vote_result.voter_id %></td>
        <td><%= vote_result.candidate_id %></td>
        <td><%= vote_result.user_id %></td>
        <td><%= vote_result.active_status %></td>
        <td><%= vote_result.del_status %></td>
        <td><%= link_to 'Show', vote_result %></td>
        <td><%= link_to 'Edit', edit_vote_result_path(vote_result) %></td>
        <td><%= link_to 'Destroy', vote_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote result', new_vote_result_path %>

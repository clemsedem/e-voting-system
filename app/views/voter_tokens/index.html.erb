<p id="notice"><%= notice %></p>

<h1>Listing Voter Tokens</h1>

<table>
  <thead>
    <tr>
      <th>Voter</th>
      <th>Token</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voter_tokens.each do |voter_token| %>
      <tr>
        <td><%= voter_token.voter_id %></td>
        <td><%= voter_token.token %></td>
        <td><%= voter_token.user_id %></td>
        <td><%= voter_token.active_status %></td>
        <td><%= voter_token.del_status %></td>
        <td><%= link_to 'Show', voter_token %></td>
        <td><%= link_to 'Edit', edit_voter_token_path(voter_token) %></td>
        <td><%= link_to 'Destroy', voter_token, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voter token', new_voter_token_path %>

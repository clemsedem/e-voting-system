<p id="notice"><%= notice %></p>

<h1>Listing Portfolio Masters</h1>

<table>
  <thead>
    <tr>
      <th>Ref</th>
      <th>Portfolio</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @portfolio_masters.each do |portfolio_master| %>
      <tr>
        <td><%= portfolio_master.ref_id %></td>
        <td><%= portfolio_master.portfolio %></td>
        <td><%= portfolio_master.user_id %></td>
        <td><%= portfolio_master.active_status %></td>
        <td><%= portfolio_master.del_status %></td>
        <td><%= link_to 'Show', portfolio_master %></td>
        <td><%= link_to 'Edit', edit_portfolio_master_path(portfolio_master) %></td>
        <td><%= link_to 'Destroy', portfolio_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Portfolio master', new_portfolio_master_path %>

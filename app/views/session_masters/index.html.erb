<p id="notice"><%= notice %></p>

<h1>Listing Session Masters</h1>

<table>
  <thead>
    <tr>
      <th>Session</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @session_masters.each do |session_master| %>
      <tr>
        <td><%= session_master.session %></td>
        <td><%= session_master.user_id %></td>
        <td><%= session_master.active_status %></td>
        <td><%= session_master.del_status %></td>
        <td><%= link_to 'Show', session_master %></td>
        <td><%= link_to 'Edit', edit_session_master_path(session_master) %></td>
        <td><%= link_to 'Destroy', session_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session master', new_session_master_path %>

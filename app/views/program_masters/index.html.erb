<p id="notice"><%= notice %></p>

<h1>Listing Program Masters</h1>

<table>
  <thead>
    <tr>
      <th>Program name</th>
      <th>Department</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @program_masters.each do |program_master| %>
      <tr>
        <td><%= program_master.program_name %></td>
        <td><%= program_master.department_id %></td>
        <td><%= program_master.user_id %></td>
        <td><%= program_master.active_status %></td>
        <td><%= program_master.del_status %></td>
        <td><%= link_to 'Show', program_master %></td>
        <td><%= link_to 'Edit', edit_program_master_path(program_master) %></td>
        <td><%= link_to 'Destroy', program_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Program master', new_program_master_path %>

<p id="notice"><%= notice %></p>

<h1>Listing Gender Masters</h1>

<table>
  <thead>
    <tr>
      <th>Gender</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gender_masters.each do |gender_master| %>
      <tr>
        <td><%= gender_master.gender %></td>
        <td><%= gender_master.user_id %></td>
        <td><%= gender_master.active_status %></td>
        <td><%= gender_master.del_status %></td>
        <td><%= link_to 'Show', gender_master %></td>
        <td><%= link_to 'Edit', edit_gender_master_path(gender_master) %></td>
        <td><%= link_to 'Destroy', gender_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gender master', new_gender_master_path %>

<p id="notice"><%= notice %></p>

<h1>Listing Department Masters</h1>

<table>
  <thead>
    <tr>
      <th>Department name</th>
      <th>Faculty</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @department_masters.each do |department_master| %>
      <tr>
        <td><%= department_master.department_name %></td>
        <td><%= department_master.faculty_id %></td>
        <td><%= department_master.user_id %></td>
        <td><%= department_master.active_status %></td>
        <td><%= department_master.del_status %></td>
        <td><%= link_to 'Show', department_master %></td>
        <td><%= link_to 'Edit', edit_department_master_path(department_master) %></td>
        <td><%= link_to 'Destroy', department_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Department master', new_department_master_path %>

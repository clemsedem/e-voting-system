				<%= @candidate_wc4.registered_voter.surname %>	<%= @candidate_wc4.registered_voter.other_names %>
				<h5>Votes: <span id="votes"><%= @count_wc4 %></span></h5>
				<h5>Percentage: <span id="percent"><%= @wc4_per_final %>%</span></h5>		   			
			</div>
		<%end%>
		
		<!-- slot 5 -->
		<% if @candidate_wc5.present? %>
			<div class="col-md-4 col-sm-12 wc3 w3-card" style="text-align: center">
			<br />			
				<% if @candidate_wc5.image.present?%>
					 <%= image_tag @candidate_wc5.image.url(:thumb), class: "img-circle ", style: "width: 100px;  margin: 2px" %>	      		  
				<%else%>
					<%= image_tag ('user.png'), class: "img-circle", style: "width: 100px;  margin: 2px" %> 
				<%end%>
				<div class="w3-clear"></div>
				<%= @candidate_wc5.registered_voter.surname %>	<%= @candidate_wc5.registered_voter.other_names %>
				<h5>Votes: <span id="votes"><%= @count_wc5 %></span></h5>
				<h5>Percentage: <span id="percent"><%= @wc5_per_final %>%</span></h5>		   			
			</div>
		<%end%>
		
		<!-- slot 6 -->
		<% if @candidate_wc6.present? %>
			<div class="col-md-4 col-sm-12 wc3 w3-card" style="text-align: center">
			<br />			
				<% if @candidate_wc6.image.present?%>
					 <%= image_tag @candidate_wc6.image.url(:thumb), class: "img-circle ", style: "width: 100px;  margin: 2px" %>	      		  

<div class="row w3-padding">
	<h4>Financial Secretary Results</h4>
	<div class="pull-left">
	<h5 style="font-family: Tahoma; font-weight: bold; font-size: 20px">Total Number of Reg. Voters: <span style="font-weight: bold; font-family: Tahoma; font-size: 30px; color: blue; margin-left: 20px "> <%= @total_reg_voters%></span> </h5>
	<h5 style="font-family: Tahoma; font-weight: bold; font-size: 20px">Total Tokens Generated: <span style="font-weight: bold; font-family: Tahoma; font-size: 30px; color: blue; margin-left: 20px "><%= @total_tokens %></span> </h5>
	<h5 style="font-family: Tahoma; font-weight: bold; font-size: 20px">Total Tokens Used: <span style="font-weight: bold; font-family: Tahoma; font-size: 30px; color: blue; margin-left: 20px "><%= @total_tokens_used %></span> </h5>
	</div>
	<div class="pull-right">
	<h5 style="font-family: Tahoma; font-weight: bold; font-size: 20px">Total Votes for Fin. Secretary: <span style="font-weight: bold; font-family: Tahoma; font-size: 30px; color: blue; margin-left: 20px "><%= @vote_cast_for_fs %></span> </h5>
	<h5 style="font-family: Tahoma; font-weight: bold; font-size: 20px">Total Valid Votes for Fin. Secretary: <span style="font-weight: bold; font-family: Tahoma; font-size: 30px; color: blue; margin-left: 20px "><%= @total_valid_votes_for_fs %> <i style="color: red"><%#= @valid_fs_final %></i></span> </h5>
	<h5 style="font-family: Tahoma; font-weight: bold; font-size: 20px">Total Rejected Votes for Fin. Secretary: <span style="font-weight: bold; font-family: Tahoma; font-size: 30px; color: blue; margin-left: 20px "><%= @total_rejected_vote_for_fs %> <i style="color: red">(<%= @reject_per_final_fs %>%)</i></span> </h5>
	</div>
	<div class="w3-clear clearfix"></div>
	<br />
	<!-- row 1 -->
	<div class="row w3-padding">
		<!-- slot 1 -->
		<% if @candidate_fs1.present? %>
		   <div class="col-md-4 col-sm-12 fs1 w3-card" style="text-align: center">
			 <br />	
				<% if @candidate_fs1.image.present?%>
					 <%= image_tag @candidate_fs1.image.url(:thumb), class: "img-circle ", style: "width: 100px;  margin: 2px" %>	      		  
				<%else%>
					<%= image_tag ('user.png'), class: "img-circle", style: "width: 100px;  margin: 2px" %> 
				<%end%>
				<div class="w3-clear"></div>
				<%= @candidate_fs1.registered_voter.surname %>	<%= @candidate_fs1.registered_voter.other_names %>		
				<h5>Votes: <span id="votes"><%= @count_fs1 %></span></h5>
				<h5>Percentage: <span id="percent"><%= @fs1_per_final %>%</span></h5>		    
			</div>
		<%end%>
		
		<!-- slot 2 -->
		<% if @candidate_fs2.present? %>
			<div class="col-md-4 col-sm-12 fs2 w3-card" style="text-align: center">
			<br />		
				<% if @candidate_fs2.image.present?%>
					 <%= image_tag @candidate_fs2.image.url(:thumb), class: "img-circle ", style: "width: 100px;  margin: 2px" %>	      		  
				<%else%>
					<%= image_tag ('user.png'), class: "img-circle", style: "width: 100px;  margin: 2px" %> 
				<%end%>
				<div class="w3-clear"></div>
				<%= @candidate_fs2.registered_voter.surname %>	<%= @candidate_fs2.registered_voter.other_names %>
				<h5>Votes: <span id="votes"><%= @count_fs2 %></span></h5>	
				<h5>Percentage: <span id="percent"><%= @fs2_per_final %>%</span></h5>			
			</div>
		<%end%>
		
		<!-- slot 3 -->
		<% if @candidate_fs3.present? %>
			<div class="col-md-4  col-sm-12 fs3 w3-card" style="text-align: center">
			<br />			
				<% if @candidate_fs3.image.present?%>
					 <%= image_tag @candidate_fs3.image.url(:thumb), class: "img-circle ", style: "width: 100px;  margin: 2px" %>	      		  

			</div>
		<%end%>
		
		<!-- slot 5 -->
		<% if @candidate_fs5.present? %>
			<div class="col-md-4 col-sm-12 fs3 w3-card" style="text-align: center">
			<br />			
				<% if @candidate_fs5.image.present?%>
					 <%= image_tag @candidate_fs5.image.url(:thumb), class: "img-circle ", style: "width: 100px;  margin: 2px" %>	      		  

<p id="notice"><%= notice %></p>

<h1>Listing Faculty Masters</h1>

<table>
  <thead>
    <tr>
      <th>Faculty name</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @faculty_masters.each do |faculty_master| %>
      <tr>
        <td><%= faculty_master.faculty_name %></td>
        <td><%= faculty_master.user_id %></td>
        <td><%= faculty_master.active_status %></td>
        <td><%= faculty_master.del_status %></td>
        <td><%= link_to 'Show', faculty_master %></td>
        <td><%= link_to 'Edit', edit_faculty_master_path(faculty_master) %></td>
        <td><%= link_to 'Destroy', faculty_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Faculty master', new_faculty_master_path %>
